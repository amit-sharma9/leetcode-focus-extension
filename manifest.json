{
    "manifest_version": 3,
    "name": "Leetcode Focus Mode",
    "version": "1.0",
    "description": "Hides LeetCode problem metadata to reduce pressure while solving.",
    "permissions":["scripting" , "storage","tabs"],
    "host_permissions" : ["https://leetcode.com/*"],
    "action" : {
        "default_popup" : "popup.html",
        "default_icon": "icon.png"
    },
    "content_scripts" : [
        {
            "matches":["https://leetcode.com/*"],
            "js" : ["content.js"]
        }
    ],
    "icons": {
  "16": "icon.png",
  "48": "icon.png",
  "128": "icon.png"
}
    
}